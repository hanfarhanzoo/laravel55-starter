{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "akaunting/setting": "^1.0",
    "anahkiasen/former": "^4.1",
    "arrilot/laravel-widgets": "^3.9",
    "balping/laravel-hashslug": "^1.1",
    "bepsvpt/secure-headers": "^4.1",
    "davejamesmiller/laravel-breadcrumbs": "^4.2",
    "doctrine/dbal": "^2.6",
    "eusonlito/laravel-meta": "^3.1",
    "eusonlito/laravel-packer": "^2.1",
    "fideloper/proxy": "~3.3",
    "garygreen/pretty-routes": "^1.0",
    "jenssegers/agent": "^2.6",
    "kingofcode/laravel-uploadable": "^0.1.7",
    "lab404/laravel-impersonate": "^1.2",
    "laracasts/flash": "^3.0",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "~1.0",
    "mews/purifier": "^2.0",
    "nwidart/laravel-modules": "^2.6",
    "olssonm/l5-very-basic-auth": "^4.1",
    "recca0120/terminal": "^1.6",
    "sarfraznawaz2005/applog": "^1.0",
    "sarfraznawaz2005/eventable": "^1.0",
    "sarfraznawaz2005/visitlog": "^1.0",
    "spatie/laravel-permission": "^2.7",
    "themsaid/laravel-mail-preview": "^2.0",
    "uxweb/sweet-alert": "^1.4",
    "vakata/websocket": "^1.0",
    "watson/active": "^2.0",
    "watson/validating": "^3.1",
    "yajra/laravel-datatables-buttons": "^3.1",
    "yajra/laravel-datatables-oracle": "~8.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "beyondcode/laravel-query-detector": "^0.1.1",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "imanghafoori/laravel-anypass": "^1.0",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover",
      "@php artisan vendor:cleanup"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
